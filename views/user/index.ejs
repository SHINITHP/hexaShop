                                <% if(item.CategoryName=="Mens" && item.Inventory==="Listed" ){ %>
                                    <div class="item">
                                        <div class="thumb">
                                            <div class="hover-content">
                                                <ul>
                                                    <li><a href="/productOverview?id=<%= item._id %>"><i
                                                                class="fa fa-eye"></i></a>
                                                    </li>
                                                    <%
                                                    let isWishlisted = false;
                                                    productID.forEach(id => {
                                                        if (id.toString() === item._id.toString()) {
                                                            isWishlisted = true;
                                                        }
                                                    });
                                                    %>
                                                   
                                                
                                                    <li id="removeWishlisted<%=index%>">
                                                        <a id="RemoveWishlist<%=index%>" data-index="<%=index%>" onclick="Wishlist(this,'<%=item._id%>')">
                                                            <i style="color: red;cursor: pointer; <%= isWishlisted ? '' : 'display: none;' %>" class="fa fa-heart remove"></i>
                                                            <i class="fa fa-heart add" style="cursor: pointer; <%= isWishlisted ? 'display: none;' : '' %>"></i>
                                                        </a>
                                                    </li>
                                                    
                                                </ul>
                                            </div>
                                            <img src=<%=item.files[1].url %> alt="">
                                        </div>
                                        <div class="down-content">
                                            <h4>
                                                <%= item.ProductName %>
                                            </h4>
                                            <span >
                                                <%  if(item.offer > 0) { %>
                                                    ₹ <%= item.SalesRate - item.SalesRate * item.offer / 100 %> <b style="font-weight: 500;text-decoration: line-through;"><%=item.MRP%></b>
                                                    <b style="color: black; padding-left: 10px;font-size: 15pt;"><%=item.offer%>%OFF</b>
                                                <% } else{ %>
                                                    <%= item.SalesRate %> <b style="font-weight: 500;text-decoration: line-through;"><%=item.MRP%></b>
                                                <% } %>
                                               
                                            </span>
                                            <ul class="stars">
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star"></i></li>
                                            </ul>
                                        </div>
                                    </div>
                                    <% } %>
